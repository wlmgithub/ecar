

def doit(a):
    i, j = 0, len(a) - 1
    while i < j:
        print(i, j)
        if a[i] == 0 and a[j] != 0:
            a[i], a[j] = a[j], a[i]
            i += 1
            j -= 1
        elif a[i] != 0 and a[j] == 0:
            i += 1
            j -= 1
        else:
            i += 1


if __name__ == '__main__':
    a = [1,0,2,12,0,-1]
    print(a)
    doit(a)
    print(a)
